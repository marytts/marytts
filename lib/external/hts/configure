set dummy excite; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_EXCITE+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $EXCITE in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_EXCITE="$EXCITE" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
as_dummy="${sptk_search_path}:$PATH"
for as_dir in $as_dummy
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_EXCITE="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_path_EXCITE" && ac_cv_path_EXCITE=":"
  ;;
esac
fi
EXCITE=$ac_cv_path_EXCITE
if test -n "$EXCITE"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $EXCITE" >&5
$as_echo "$EXCITE" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


if test ! -f "$EXCITE"; then
	as_fn_error $? "Can't find excite of SPTK" "$LINENO" 5
fi

