#Auto-generated config file for voice hsmm-slt

name = en_US-hsmm-slt
en_US-voice.version = 4.0

voice.version = 4.0

# Declare "group names" as component that other components can require.
# These correspond to abstract "groups" of which this component is an instance.
provides = \
         en_US-voice \
         hmm-voice

# List the dependencies, as a whitespace-separated list.
# For each required component, an optional minimum version and an optional
# download url can be given.
# We can require a component by name or by an abstract "group name"
# as listed under the "provides" element.
requires = \
   en_US \
   marybase


requires.marybase.version = 4.0.0
requires.marybase.download = http://mary.dfki.de/download/mary-install-4.x.x.jar
requires.en_US.version = 4.0.0
requires.en_US.download = http://mary.dfki.de/download/mary-install-4.x.x.jar
requires.hmm.version = 4.0.0

####################################################################
####################### Module settings  ###########################
####################################################################
# For keys ending in ".list", values will be appended across config files,
# so that .list keys can occur in several config files.
# For all other keys, values will be copied to the global config, so
# keys should be unique across config files.

hmm.voices.list = \
   hsmm-slt

# If this setting is not present, a default value of 0 is assumed.
voice.hsmm-slt.wants.to.be.default = 0

# Set your voice specifications
voice.hsmm-slt.gender = female
voice.hsmm-slt.locale = en_US
voice.hsmm-slt.domain = general
voice.hsmm-slt.samplingRate = 16000

# HMM Voice-specific parameters 
# parameters used during models training 
# MGC: stage=gamma=0 alpha=0.42 linear gain (default) 
# LSP: gamma>0  
#          LSP: gamma=1 alpha=0.0  linear gain/log gain 
#      Mel-LSP: gamma=1 alpha=0.42 log gain 
#      MGC-LSP: gamma=3 alpha=0.42 log gain 
voice.hsmm-slt.alpha = 0.42
voice.hsmm-slt.gamma = 0
voice.hsmm-slt.logGain = false

# Parameter beta for postfiltering 
voice.hsmm-slt.beta = 0.0

# HMM Voice-specific files
# Information about trees
voice.hsmm-slt.Ftd = MARY_BASE/lib/voices/hsmm-slt/tree-dur.inf
voice.hsmm-slt.Ftf = MARY_BASE/lib/voices/hsmm-slt/tree-lf0.inf
voice.hsmm-slt.Ftm = MARY_BASE/lib/voices/hsmm-slt/tree-mgc.inf
voice.hsmm-slt.Fts = MARY_BASE/lib/voices/hsmm-slt/tree-str.inf
voice.hsmm-slt.Fta = MARY_BASE/lib/voices/hsmm-slt/tree-mag.inf

# Information about means and variances PDFs 
voice.hsmm-slt.Fmd = MARY_BASE/lib/voices/hsmm-slt/dur.pdf
voice.hsmm-slt.Fmf = MARY_BASE/lib/voices/hsmm-slt/lf0.pdf
voice.hsmm-slt.Fmm = MARY_BASE/lib/voices/hsmm-slt/mgc.pdf
voice.hsmm-slt.Fms = MARY_BASE/lib/voices/hsmm-slt/str.pdf
voice.hsmm-slt.Fma = MARY_BASE/lib/voices/hsmm-slt/mag.pdf

# Information about Global Mean and Variance PDFs 
# By default GV is not used for generating strengths and Fourier magnitudes,
# although the gv pdf for these are generated during training. 
# Uncomment the lines corresponding to gv-str and gv-mag for using them.
voice.hsmm-slt.useGV = true
voice.hsmm-slt.Fgvf = MARY_BASE/lib/voices/hsmm-slt/gv-lf0-littend.pdf
voice.hsmm-slt.Fgvm = MARY_BASE/lib/voices/hsmm-slt/gv-mgc-littend.pdf
#voice.hsmm-slt.Fgvs = MARY_BASE/lib/voices/hsmm-slt/gv-str-littend.pdf
#voice.hsmm-slt.Fgva = MARY_BASE/lib/voices/hsmm-slt/gv-mag-littend.pdf

# File for testing the HMMSynthesiser, a context features file example.
voice.hsmm-slt.FeaFile = MARY_BASE/lib/voices/hsmm-slt/arctic_a0001.pfeats

# Information about Mixed Excitation
voice.hsmm-slt.useMixExc = true
voice.hsmm-slt.useFourierMag = true

# Information for using external prosody, if set to true it will use the MARY targetfeatures: 
# ContinuousFeatureProcessors
#  unit_duration float 
#  unit_logf0 float 
#  unit_logf0delta float
voice.hsmm-slt.useExtDur = false
voice.hsmm-slt.useExtLogF0 = false

#voice.hsmm-slt.duration.cart = MARY_BASE/lib/voices/slt-arctic/dur.graph.mry
#voice.hsmm-slt.f0.graph = MARY_BASE/lib/voices/slt-arctic/f0contourtree.mry
#voice.hsmm-slt.f0.contours = MARY_BASE/lib/voices/slt-arctic/syllableF0Polynomials.mry



# Filter taps of bandpass filters for mixed excitation 
# File format: for example if we have 5 filters each with 48 taps 
# then the taps are in a vector 
# tap[1][1] 
# ... 
# tap[1][48] 
# tap[2][1] 
# ... 
# tap[2][48] 
# ... 
# tap[5][1] 
# ... 
# tap[5][48] 
voice.hsmm-slt.Fif = MARY_BASE/lib/voices/hsmm-slt/mix_excitation_filters.txt
# Number of filters in bandpass bank, default 5 filters 
voice.hsmm-slt.in = 5
# Number of taps in bandpass filters, default 48 taps 
voice.hsmm-slt.io = 48
